drop table entries;
create table entries(sec smallint, title varchar(50), text varchar(200));
insert into entries values(1, 'title question 1.1', 'text question 1.1');
insert into entries values(1, 'title question 1.2', 'text question 1.2');
insert into entries values(2, 'title question 2.1', 'text question 2.1');
insert into entries values(2, 'title question 2.2', 'text question 2.2');
select * from entries;
